/* 已发布的需求列表 */





/* 已发布的产品列表 */

.productList {
  padding: 30rpx 20rpx 60rpx 20rpx;
  display: flex;
  flex-wrap: wrap;
}


.color{
  color:#FF6901 !important;
}


/* 删除弹窗 */

.deleteBack {
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.49);
  position: fixed;
  top: 0;
  z-index: 2;
}

.deleteTier {
  width: 590rpx;
  height: 290rpx;
  background-color: #fff;
  position: fixed;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  border-radius: 8px;
}

.deleteTitle{
  font-weight: bold;
  padding-top: 50rpx;
  padding-bottom: 40rpx;
  display: block;
  text-align: center;
}

