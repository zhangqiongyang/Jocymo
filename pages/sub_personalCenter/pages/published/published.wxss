/* pages/sub_details/pages/published/published.wxss */

/* 已发布的分类 */

.publishedClassify {
  width: 100%;
  padding: 0 30rpx;
  height: 88rpx;
  display: flex;
  background-color: #fff;
  box-sizing: border-box;
}

.classify {
  height: 100%;
  flex: 1;
  display: flex;
  align-items: center;
  justify-content: center;
  box-sizing: border-box;
  color: #666;
}

.classifyHover {
  height: 100%;
  flex: 1;
  display: flex;
  align-items: center;
  justify-content: center;
  box-sizing: border-box;
  color: #ff6901;
  border-bottom: 4rpx solid #ff6901;
}

